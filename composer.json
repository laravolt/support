{
    "name": "Laravolt/Support",
    "description": "Laravolt support",
    "keywords": [
        "laravolt",
        "support"
    ],
    "homepage": "https://github.com/laravolt/support",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "illuminate/support": "~5.1",
        "edvinaskrucas/notification": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Support\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
